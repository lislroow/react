{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "src",
    "paths": {
      "*": [
        "*"
      ]
      // "@/components/*": ["components/*"],
      // "@/lib/*": ["lib/*"],
      // "@/services/*":["services/*"],
      // "@/styles/*": ["styles/*"],
      // "@/types/*":["types/*"],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    // "strict": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "types": [
      "@types/styled-components",
      "node",
      "styled-jsx"
    ],
    "incremental": true
  },
  "include": [
    "**/*.js",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
